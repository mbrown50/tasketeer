     <header id="home">
      Taskateer Scheduler
    <nav class="navbar" role="navigation" aria-label="main navigation" >
      <div class="navbar-brand">
     <!-- <div class="navbar-menu"> -->
        <div class="navbar-start" >
          <!-- navigation links -->
            <a class="navbar-item is-underlined" href="#about">
            About Us
          </a>
           <button id="user-button" class="input-button btn">View User</button>
         <button id="logout-button" class="input-button btn">Logout Task</button>
          <button id="input-button" class="input-button btn">Add Task</button>
                <div id="input-modal" class="input-modal" >
                    <div >
                        <span class="close">&times;</span>
                        <form class="input-modal-content">
                            <label for="input-title">Input task:</label>
                            <input type="text" id="input-title"><br>
                            <label for="input-date">Input date:</label>
                            <input type="text" id="input-date"><br>
                            <label for="input-time">Input time:</label>
                            <input type="text" id="input-time"><br>
                            <label for="input-location">Input location:</label>
                            <input type="text" id="input-location"><br>
                            <label for="input-notes">Input notes:</label>
                            <input type="text" id="input-notes"><br>
                            <input id="input-submit" type="submit" value="Submit">
                        </form>
                    </div>
                </div>
        </div>
         </div>
    </nav>
  </header>
