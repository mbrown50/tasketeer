    <div id="cards-group">
                {{#each cardSet as |card|}}
                    <div class="card card-body block container">
                        <h5 class="card-title">{{card.task}}</h5>
                        <p class="card-text">Datetime: {{card.datetime}}</p>
                        <p class="card-text">Location: {{card.location}}</p> 
                        <p class="card-text">Notes: {{card.notes}}</p>               
                    </div>
                {{/each}}
                
            </div>
              <section >
      <div class="box" id="shadow-box">
        <div class="box is-large container" id="about">
